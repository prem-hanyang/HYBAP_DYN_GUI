                        </TextBox>
                        <TextBlock Grid.Column="5" Margin="0" TextAlignment="Right" Text="[" />
                        <TextBlock Grid.Column="6" Margin="0" TextAlignment="Center" Text="-" />
                        <TextBlock Grid.Column="7" Margin="0" TextAlignment="Left" Text="]" />
                    </Grid>
                    <Grid Grid.Row="11">
