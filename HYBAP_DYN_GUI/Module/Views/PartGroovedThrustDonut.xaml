                        <TextBlock Grid.Column="1" Margin="0" TextAlignment="Left" Text="Ratio of groove to ridge, dg/(dg+dr) :" />
                        <TextBox Grid.Column="3" TextAlignment="Center">
                            <TextBox.Text>
